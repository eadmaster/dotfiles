echo "$0: PSRewired (PS2, PS3, PSP)  https://psrewired.com"
curl -s -m3 https://api.psrewired.com/us/api/universes?applicationId=20764 | jq -c '.[] | select(.playerCount > 0) | pick(.name, .description, .playerCount)'   # Motorstorm NTSC (PS3)
curl -s -m3 https://api.psrewired.com/us/api/universes?applicationId=20774 | jq -c '.[] | select(.playerCount > 0) | pick(.name, .description, .playerCount)'   # Ratchet & Clank: Size Matters NTSC/PAL (PS2/PSP)
curl -s -m3 https://api.psrewired.com/us/api/universes?applicationId=10694 | jq -c '.[] | select(.playerCount > 0) | pick(.name, .description, .playerCount)'   # Twisted Metal: Head-On (PS2/PSP)

